<IfModule mod_rewrite.c>
    
	#Bloquear listagem
	Options -Indexes

	Options +FollowSymLinks
	RewriteEngine on

	DefaultLanguage pt-BR
	AddDefaultCharset UTF-8

	RewriteCond %{SCRIPT_FILENAME} !-d
	RewriteCond %{SCRIPT_FILENAME} !-f

	#PAGES**************************************************************
	
  	RewriteRule ^cupons/(.*)?$ cupom.php?id_cadastro=$1 [L,QSA]
	RewriteRule ^dados/(.*)?$ dados.php?id_cadastro=$1 [L,QSA]
	RewriteRule ^logout?$ controlers/logout.php [L,QSA]

	RewriteRule ^home?$ index.php [L,QSA]

	RewriteRule ^cupom?$ cupom.php [L,QSA]
	RewriteRule ^meus-dados?$ dados.php [L,QSA]
	
</IfModule>


RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}

